{
  "name": "win-screen-resolution",
  "version": "2.0.0",
  "description": "Get the current and all available screen resolution on Windows",
  "type": "module",
  "exports": "./lib/esm.js",
  "types": "./types/esm.d.ts",
  "os": [
    "win32"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "node-gyp rebuild",
    "prebuild": "prebuildify --target node@ --napi --platform win32 --arch x64",
    "clean": "node-gyp clean",
    "lint": "npx eslint --ignore-path .gitignore ./**/*.{js,mjs}",
    "test": "tap test/auto/* --no-coverage --no-coverage-report",
    "install": "node-gyp-build"
  },
  "keywords": [
    "screen",
    "resolution",
    "windows",
    "DPI"
  ],
  "author": {
    "name": "Anthony Beaumont",
    "email": "dev.xan105@outlook.be",
    "url": "https://xan105.com/"
  },
  "license": "MIT",
  "gypfile": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xan105/node-win-screen-resolution.git"
  },
  "bugs": {
    "url": "https://github.com/xan105/node-win-screen-resolution/issues"
  },
  "homepage": "https://github.com/xan105/node-win-screen-resolution#readme",
  "devDependencies": {
    "eslint": "^8.1.0",
    "node-gyp": "^8.4.0",
    "prebuildify": "^5.0.0",
    "tap": "^15.0.10"
  },
  "dependencies": {
    "@xan105/is": "^2.0.0",
    "node-addon-api": "^4.2.0",
    "node-gyp-build": "^4.3.0"
  }
}
